    <script>
      $(function() {
        CabinetNav.init();
        setTimeout(hideSocial, 2000);
        $(document).on('click', function(e) { CabinetNav.click(e.target); });
      });

      function hideSocial() {
        if (typeof (ODKL) == 'undefined' && document.getElementById("odnTd"))
          document.getElementById("odnTd").style.display = "none";
        if (typeof (VK) == 'undefined' && document.getElementById("vkTd"))
          document.getElementById("vkTd").style.display = "none";
      }

      CabinetNav = {
        sNav: null,
        sLink: null,

        init: function() {
          CabinetNav.sNav = 'cabinet-sub-nav',
          CabinetNav.sLink = 'js-show-sub-nav';
        },
        click: function(elem) {
          if ($('#' + CabinetNav.sNav).is(':visible') && $(elem).closest('#' + CabinetNav.sNav).length == 0) {
            $('#' + CabinetNav.sNav).hide();
          } else {
            if (elem.id == CabinetNav.sLink) {
              if ($('#' + CabinetNav.sNav).is(':hidden'))
                $('#' + CabinetNav.sNav).show();
            }
          }
        }
      };

      function smartphoneDataToHtml(smartphoneData) {
        var description = "";

        smartphoneData.Description.forEach(function(descriptionItem) {
          description += "<li>" + descriptionItem + "</li>";
        });

        return "<div class='product-item'>" +
                 "<a href='" + smartphoneData.Url + "' class='product-item__image'>" +
                   "<img src='" + smartphoneData.ImageUrl + "' />" +
                 "</a>" +
                 "<h3>" + smartphoneData.Name + "</h3>" +
                 "<ul>" + description + "</ul>" +
                 "<div class='product-item__price'>" + smartphoneData.Price + " <small>руб.</small></div>" +
                 "<a href='" + smartphoneData.Url + "' class='buy-btn'>Купить</a>" +
               "</div>";
      }

      (function() {
        var productsWrapper = $(".products-wrap");

        productsWrapper.hide();

        $.ajax({
          type: "GET",
          url: "/shopSearchHelper_vs.aspx",
          data: {
            isGetSmartphonesFor404: 1
          },
          dataType: "json",
          success: function(data) {
            if (data.length !== 0) {
              data.forEach(function(smartphoneData) {
                productsWrapper.append(smartphoneDataToHtml(smartphoneData));
              });

              productsWrapper.show();
            } else {
              console.warn("Не было получено ни одного смартфона от http://www.shop.mts.ru/!");
            }
          }
        });
      })();
    </script>
